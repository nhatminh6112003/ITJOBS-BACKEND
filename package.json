{
	"name": "nodejs_basic",
	"version": "1.0.0",
	"description": "",
	"main": "src/index.js",
	"scripts": {
		"start": "nodemon  --exec ./node_modules/.bin/babel-node --inspect server.js",
		"watch": "node-sass -w src/resources/scss/app.scss -o src/public/css/app.css",
		"test": "echo \"Error: no test specified\" && exit 1",
		"db": "cd src && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
		"model:create": "cd src && cross-env npx sequelize-cli model:generate --name $npm_config_name --attributes $npm_config_attr",
		"migration": "cd src && npx sequelize-cli db:migrate",
		"migration:revert": "cd src && npx sequelize-cli db:migrate:undo",
		"migration:skeleton": "cd src && npx sequelize-cli migration:generate --name $npm_config_name",
		"seed": "cd src && npx sequelize-cli db:seed:all",
		"seed:one": "cd src && cross-env npx sequelize-cli db:seed --seed $npm_config_name",
		"seed:create": "cd src && cross-env npx sequelize-cli seed:generate --name $npm_config_name",
		"lint:fix": "eslint --fix ./src/**/*.js",
		"prettier:fix": "prettier --write ."
	},
	"imports": {
		"@config/*": "./src/config/*"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@joi/date": "^2.1.0",
		"apicache": "^1.6.3",
		"bcrypt": "^5.1.0",
		"cloudinary": "^1.33.0",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-async-handler": "^1.2.0",
		"express-handlebars": "^6.0.6",
		"express-session": "^1.17.3",
		"helmet": "^6.0.1",
		"http-errors": "^2.0.0",
		"joi": "^17.9.1",
		"jsonwebtoken": "^8.5.1",
		"multer": "^1.4.5-lts.1",
		"multer-storage-cloudinary": "^4.0.0",
		"nodemon": "^2.0.20",
		"puppeteer": "^19.4.1",
		"sequelize": "^6.32.0",
		"slugify": "^1.6.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.19.3",
		"@babel/core": "^7.20.5",
		"@babel/node": "^7.20.5",
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"babel-plugin-module-resolver": "^4.1.0",
		"eslint": "^8.44.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"morgan": "^1.10.0",
		"mysql2": "^2.2.5",
		"prettier": "^2.8.1",
		"sequelize-cli": "^6.6.1"
	}
}
